{
  "name": "waiting-to-do",
  "private": true,
  "version": "0.0.0",
  "description": "WaitingToDo",
  "author": "blueocean",
  "main": "./out/main/index.js",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "vite preview",
    "dev:web": "vite",
    "electron:preview": "electron-vite preview",
    "build:app": "electron-builder"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "element-plus": "^2.9.10",
    "path": "^0.12.7",
    "pinia": "^3.0.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^22.15.18",
    "@vitejs/plugin-vue": "^5.2.3",
    "electron": "^37.2.1",
    "electron-builder": "^26.0.12",
    "electron-vite": "^3.1.0",
    "vite": "^6.3.5"
  },
  "build": {
    "appId": "com.blue.waitingtodo",
    "productName": "WaitingToDo",
    "files": [
      "out/main/**/*",
      "out/renderer/**/*",
      "resources/**/*",
      "!node_modules/**/*"
    ],
    "asar": true,
    "win": {
      "target": "nsis",
      "icon": "./resources/icon.ico",
      "requestedExecutionLevel": "asInvoker"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  }
}
